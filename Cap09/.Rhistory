# Desenhando a distribuição de probabilidades
graph <- function(n,p){
x <- dbinom(0:n, size = n, prob = p)
barplot(x,ylim=c(0,1),names.arg=0:n,
main=sprintf(paste('Distribuição Binomial (n,p) ',n,p,sep=',')))
}
graph(5,0.04)
